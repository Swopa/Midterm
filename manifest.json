{
    "manifest_version": 3,
    "name": "Flashcard Quick Creator",
    "version": "0.1",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["dist/content_script.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }